<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 192 192" width="192" height="192">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6366F1;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#8B5CF6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#A855F7;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="docGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F8FAFC;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="textGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#A855F7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#6366F1;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="0" dy="4" stdDeviation="8" flood-color="#000000" flood-opacity="0.1"/>
    </filter>
    <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background circle with enhanced gradient and shadow -->
  <circle cx="96" cy="96" r="90" fill="url(#bgGradient)" stroke="#1E3A8A" stroke-width="3" filter="url(#shadow)"/>
  
  <!-- Main document with gradient -->
  <rect x="42" y="30" width="108" height="132" rx="12" fill="url(#docGradient)" stroke="#E2E8F0" stroke-width="2"/>
  
  <!-- Document header with gradient -->
  <rect x="54" y="48" width="84" height="12" fill="url(#textGradient)" rx="3"/>
  
  <!-- Content sections with gradient -->
  <rect x="54" y="72" width="72" height="6" fill="#64748B" rx="2"/>
  <rect x="54" y="84" width="60" height="6" fill="#94A3B8" rx="2"/>
  <rect x="54" y="96" width="66" height="6" fill="#94A3B8" rx="2"/>
  <rect x="54" y="108" width="48" height="6" fill="#94A3B8" rx="2"/>
  <rect x="54" y="120" width="54" height="6" fill="#94A3B8" rx="2"/>
  
  <!-- Second content section -->
  <rect x="54" y="138" width="78" height="6" fill="#64748B" rx="2"/>
  <rect x="54" y="150" width="64" height="6" fill="#94A3B8" rx="2"/>
  <rect x="54" y="162" width="70" height="6" fill="#94A3B8" rx="2"/>
  
  <!-- Corner fold with gradient -->
  <path d="M 138 30 L 150 42 L 138 42 Z" fill="#F1F5F9"/>
  
  <!-- Resume4Me branding with gradient and glow -->
  <text x="96" y="180" font-family="Arial, sans-serif" font-size="18" font-weight="bold" text-anchor="middle" fill="url(#textGradient)" filter="url(#glow)">Resume4Me</text>
  
  <!-- Subtle decorative elements with gradient -->
  <circle cx="72" cy="60" r="2" fill="#FFFFFF" opacity="0.9"/>
  <circle cx="96" cy="186" r="1" fill="url(#textGradient)" opacity="0.8"/>
  <circle cx="84" cy="186" r="1" fill="#64748B" opacity="0.4"/>
  <circle cx="108" cy="186" r="1" fill="#64748B" opacity="0.4"/>
</svg>
